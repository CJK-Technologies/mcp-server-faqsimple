{
  "name": "mcp-server-faqsimple",
  "version": "1.0.0",
  "description": "MCP Server for FAQsimple - Enable AI assistants to access and reference your FAQ content through the Model Context Protocol",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "faqsimple",
    "ai",
    "assistant",
    "faq",
    "knowledge-base",
    "claude",
    "server"
  ],
  "main": "dist/index.js",
  "bin": {
    "mcp-server-faqsimple": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/mcp-server-faqsimple.git"
  },
  "author": "Your Organization",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/your-org/mcp-server-faqsimple/issues"
  },
  "homepage": "https://github.com/your-org/mcp-server-faqsimple#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}